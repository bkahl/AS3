package com.potiondesign {		import flash.events.*;		public class WallPointsEvent extends Event {				static public const GOTPOINTS:String = "got_points";		static public const ERROR:String = "potion error";		static public const CONNECT:String = "potoin connect";		public var numPoints:uint;		public var pointList:Array;				public var parameters:Object;				public function WallPointsEvent(eventName:String, _param:Object = null) {			super(eventName, false); // no bubbles						parameters = _param;						if (parameters.pointList != null){						this.numPoints = parameters.numPoints;				this.pointList = parameters.pointList.slice();			}		}				/*override public function clone():Event {			return new WallPointsEvent(numPoints, pointList);		}*/				override public function toString():String {			//return formatToString("WallPointsEvent", "type", "bubbles", "cancelable", "eventPhase");			return "[numPoints:"+numPoints+"]";		}	}}