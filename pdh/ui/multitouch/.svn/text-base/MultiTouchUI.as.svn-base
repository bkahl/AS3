/**	movieclips or sprites which extend this class will gain access to events passed along 	from multi touch technologies.  The events dispatched are:		MultiTouchEvent.PRESS		MultiTouchEvent.RELEASE		// roll over and roll out events do not exist*/package pdh.ui.multitouch{	import flash.display.Sprite;			import flash.events.Event;	import flash.utils.Dictionary;	import flash.events.MouseEvent;	import pdh.ui.UIObject;	import pdh.ui.multitouch.MultiTouchEvent;	import pdh.potion.PotionProxy;		public class MultiTouchUI extends UIObject	{			public static var multiTouchProxy:PotionProxy = PotionProxy.getInstance()			public function MultiTouchUI()		{							// init singleton pattern for xml socket			multiTouchProxy.registerDisplayObject(this);		}							public function pressed():void 		{			// dispatch event for down event			dispatchEvent(new MultiTouchEvent(MultiTouchEvent.PRESS, this	))		}						public function released():void 		{			// dispatch event when no longer pressed			dispatchEvent(new MultiTouchEvent(MultiTouchEvent.RELEASE, this  ))		}				}}