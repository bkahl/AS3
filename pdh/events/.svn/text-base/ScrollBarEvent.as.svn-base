package pdh.events {	import flash.events.Event;		public class ScrollBarEvent extends Event {		public static const UPDATE:String = "update";		public static const NEXT:String = "next";		public static const PREV:String = "prev";		public var scroll_value:Number;				public function ScrollBarEvent( _type:String, _value:Number ) {			super( _type );			scroll_value = _value;		}	}}