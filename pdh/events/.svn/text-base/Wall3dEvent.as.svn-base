package pdh.events{	import flash.events.Event;	public class Wall3dEvent extends Event 	{				public static const IMAGE_ZOOMED	:String = "image zoomed";		public static const IMAGE_ROLLOVER	:String = "image rollover";		public static const IMAGE_ROLLOUT 	:String = "image rollout";		public static const IMAGE_CLICK		:String = "image click";		public static const INTRO_DONE		:String = "intro done";				public var parameters				:Object;				public function Wall3dEvent(command:String, _data:Object = null) {			super(command);						parameters = _data;		}	}}