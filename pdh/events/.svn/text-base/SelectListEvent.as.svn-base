package pdh.events {	import flash.events.Event;	import flash.display.DisplayObject;		public class SelectListEvent extends Event {				public static const SELECTED		:String = "list selected";		public static const ROLLOVER		:String = "list item rollover";		public static const ROLLOUT			:String = "list item rollout";		public static const DESELECTED		:String = "list deselected";		public var id						:String;		public var group					:String;		public var displayObject			:DisplayObject;				public function SelectListEvent( _type:String, _id:String, _group:String, _clip:DisplayObject = null ) {			super( _type, true );			id = _id;			group = _group;			displayObject = _clip		}	}}