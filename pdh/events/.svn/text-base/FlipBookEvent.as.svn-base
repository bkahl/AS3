package pdh.events{	import flash.events.Event;		public class FlipBookEvent extends Event 	{				public static const LOADED	:String = "fb loaded";		public static const FLIPPED	:String = "fb flipped";		public static const LOADING	:String = "fb loading";		public static const VISIBLE	:String = "fb vis";				public var parameters			:Object;				public function FlipBookEvent(command:String, _param:Object = null) {			super(command, true);					parameters = _param;		}	}}