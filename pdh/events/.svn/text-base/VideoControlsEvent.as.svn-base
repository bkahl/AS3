package pdh.events{	import flash.events.Event;	public class VideoControlsEvent extends Event	{			public static const	PLAY			:String = "play";		public static const	PAUSE			:String = "pause";		public static const	SCRUB			:String = "scrub";		public static const	VOLUME			:String = "volume";		public static const	FULL_SCREEN		:String = "fullscreen";		public static const	SCRUB_STARTED	:String = "scrub start";		public static const	SCRUB_STOPPED	:String = "scrub stop";		public static const	SCRUB_UPDATE	:String = "scrub update";				public var parameters				:Object				public function VideoControlsEvent(val:String, _param:Object = null)		{			super(val, false); // bubbles			parameters = _param		}		}}