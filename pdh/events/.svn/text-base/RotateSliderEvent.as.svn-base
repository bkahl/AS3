package pdh.events{	import flash.events.Event;	public class RotateSliderEvent extends Event 	{				public static const START_SLIDE		:String = "start slide";		public static const STOP_SLIDE		:String	= "stop slide";				public var speed					:Number;				public function RotateSliderEvent(command:String, _speed:Number=.01) {			super(command);						speed = _speed;		}	}}