//import pdh.accessibility.Accessablepackage pdh.accessibility{	import flash.accessibility.Accessibility;	import flash.accessibility.AccessibilityProperties	public class Accessable	{				public static function setText(obj:*, _name:String, _descript:String =""):void 		{			var accessProps:AccessibilityProperties = new AccessibilityProperties();			accessProps.name = "Return to Intro";						if (_descript != ""){				accessProps.description = _descript			}						obj.accessibilityProperties = accessProps;		}				public static function update():void 		{			if(Accessibility.active) {                Accessibility.updateProperties();            }		}							}			}