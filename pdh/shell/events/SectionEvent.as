package pdh.shell.events{	import flash.events.Event;	public class SectionEvent extends Event	{				// loading		public static const	LOADING			:String = "section loading";		public static const	COMPLETE		:String = "section complete";		public static const	LOAD_SECTION	:String = "section load";		public static const	LOAD_ERROR		:String = "load error";				// history		public static const	BACK			:String = "section back";		public static const	UPDATE_HISTORY	:String = "update_history";				// ui		public static const ROLLOVER_BUBBLE	:String = "rollover bubble";		public static const ROLLOUT_BUBBLE	:String = "rollout bubble";		public static const MODAL			:String = "modal";				public var parameters				:Object				public function SectionEvent(val:String, _param:Object = null)		{			super(val, true); // bubbles			parameters	 = _param;		}		}}