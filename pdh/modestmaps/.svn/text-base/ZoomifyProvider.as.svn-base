package pdh.modestmaps{		import com.modestmaps.mapproviders.AbstractZoomifyMapProvider;	import com.modestmaps.mapproviders.IMapProvider;	import com.modestmaps.geo.Transformation;	import com.modestmaps.geo.LinearProjection		public class ZoomifyProvider extends AbstractZoomifyMapProvider implements IMapProvider	{						public function ZoomifyProvider(directory:String, w:Number, h:Number)		{		   super();		   		   fullWidth = w;		   fullHeight = h;				   // defineImageProperties() must be called!		   defineImageProperties(directory, w, h);		   		  var t:Transformation = new Transformation(1, 0, 0, 													0, 1, 0);		   		   __projection = new LinearProjection(0, t);		}						public override function toString():String		{			return "ZOOMIFY PROVIDER " + fullWidth + " | " + fullHeight;		}			}}