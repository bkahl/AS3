package pdh.util{    import flash.net.*;	import flash.external.ExternalInterface;    public class Web    {		protected static const WINDOW_OPEN_FUNCTION : String = "window.open";				public static function getURL(url:String, window:String = "_self"):void 		{			if (ExternalInterface.available) {				ExternalInterface.call(WINDOW_OPEN_FUNCTION, url, window);			} else {				navigateToURL(new URLRequest(url), window);			}					}				public static function sendVariables(url:String, _vars:URLVariables):void 		{  			var request:URLRequest = new URLRequest(url);  			var loader:URLLoader = new URLLoader();  			loader.dataFormat = URLLoaderDataFormat.VARIABLES;  			request.data = _vars;  			request.method = URLRequestMethod.POST;  			//loader.addEventListener(Event.COMPLETE, handleComplete);  			//loader.addEventListener(IOErrorEvent.IO_ERROR, onIOError);  			loader.load(request);  			}      } // end class} // end package