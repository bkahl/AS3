package pdh.util{		public class ArrayUtil	{				public static function randomize($array:Array):Array {			var returnArray:Array = new Array();			var orgArray:Array = $array.slice();			while (orgArray.length > 0) {				var r:uint = Math.floor(Math.random()*orgArray.length);				returnArray.push(orgArray.splice(r, 1));			}			return returnArray;		}				// create array wth incremental values starting with 0		public static function initBlankArray(num:Number):Array		{			var arr:Array = new Array();				arr.length = num;			for (var i:int = 0; i < num ; i++){				arr[i] = i;			}			return arr;		}				} // end class	}  // end package