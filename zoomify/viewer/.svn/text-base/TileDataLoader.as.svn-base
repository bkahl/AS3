package zoomify.viewer
{
	import flash.display.Loader;
	
	public class TileDataLoader extends Loader
	{
		public var g:uint;
		public var t:uint;
		public var r:uint;
		public var c:uint;
		public var path:String;
		public var immortal:Boolean;
		
		public function TileDataLoader(g:uint, t:uint, r:uint, c:uint):void
		{
			super();
			this.g = g;
			this.t = t;
			this.r = r;
			this.c = c;
			immortal = false;
		}
	}
}